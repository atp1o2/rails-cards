 <h1>User Info:</h1>
<%= @user.username %><br>
<%= @user.password %><br>
<%= @user.first_name %><br>
<%= @user.last_name %><br>
<%= @user.email %><br>

<p><%= link_to 'Back to Users', users_path %></p>
<p><%= link_to 'Edit this User', edit_user_path(@user) %></p>
<p><%= link_to 'Delete this User', user_path(@user), method: :delete, data: { confirm: "Are you sure you want to delete #{@user.username} from the database?"}  %></p>
